<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="BottomSheetDialog" parent="@style/Theme.Design.BottomSheetDialog">
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowIsFloating">false</item>
        <item name="cornerSize">0dp</item>
        <item name="cornerSizeTopLeft">30dp</item>
        <item name="cornerSizeTopRight">30dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CircleImageView" parent="">
        <item name="cornerSize">50%</item>
        <item name="strokeWidth">1dp</item>
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
    </style>

    <style name="AppAutoTextCompleteStyle" parent="Base.V7.Widget.AppCompat.AutoCompleteTextView">
        <item name="android:textSize">13sp</item>
        <item name="android:fontFamily">@font/dmsans_medium</item>
    </style>

</resources>